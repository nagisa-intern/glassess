<p id="notice"><%= notice %></p>

<h1>Comic Data</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Episode</th>
      <th>Comic</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @comic_data.each do |comic_datum| %>
      <tr>
        <td><%= comic_datum.title %></td>
        <td><%= comic_datum.episode %></td>
        <td><%= comic_datum.comic %></td>
        <td><%= link_to 'Show', comic_datum %></td>
        <td><%= link_to 'Edit', edit_comic_datum_path(comic_datum) %></td>
        <td><%= link_to 'Destroy', comic_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Comic Datum', new_comic_datum_path %>
